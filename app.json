{
  "expo": {
    "name": "EmployeeApp",
    "slug": "EmployeeApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.kamal.employee"
    },
    "web": {
      "favicon": "./assets/icon.png"
    },
    "android": {
      "package": "com.kamal.employee",
      "enableDangerousExperimentalLeanBuilds": true
    },
    "entryPoint": "node_modules/expo/AppEntry.js",
    "description": "Manage your organization employees in a single app.",
    "githubUrl": "https://github.com/srisaikamal/Emplyoee-App-React-Native"
  }
}
